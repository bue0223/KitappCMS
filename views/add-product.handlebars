    <!-- // Dashboard Content -->
    <div class="dashboard-content page-products">
      <div class="progress-fixed" style="position: fixed;top:0;width: 100%;z-index: 99; box-shadow:  0px -4px 3px 6px #a7a7a7;">
      <div class="progress" style="height: 10px;border-radius: 0">
        <div class="progress-bar" role="progressbar" style="width: 0%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
      </div>
      </div>

      <div class="container">
          <div class="row-fluid">
            <div class="col-md-12 page-title">
              <h4>{{label}}</h4>
              <p>Products <i class="fas fa-chevron-right"></i> {{label}} </p>
            </div>

            <div class="col-md-7 page-content page-products--add">
              <form id="add-edit-product-form">
                <div class="form-group row">
                  <label class="col-sm-3 col-form-label">Product Name</label>
                  <div class="col-sm-9"><input maxlength="25" type="text" class="form-control completion" id="product_name" required>
                  <div class="feedback">This is a required field | Max of (25) Characters Only</div>
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-sm-3 col-form-label">Product Sequence</label>
                  <div class="col-sm-9">
                    <select class="form-control select-picker completion_c" id="product-position">
                      <option disabled>Choose product position</option>
                      {{#each availableOrder  }}
                      <option selected value="{{this}}">{{this}}</option>
                      {{/each}}
                    </select>
                    <div class="feedback">This  is a required field</div>
                  </div>
                  </div>

                <div class="form-group row">
                  <label class="col-sm-3 col-form-label">Upload Image</label>
                  <div class="col-sm-9 file-upload">
                    <button class="btn btn-secondary" type="button" onclick="$('.file-upload-input').trigger( 'click' )">{{image_label}}</button>
                    <!-- Add "is-invalid" class for error -->
                    <div class="image-upload-wrap" id="invalid-image">
                    <!-- <div class="image-upload-wrap"> -->
                      <input class="file-upload-input completion_c" type='file' id="product_image" onchange="readURL(this);" accept="image/jpg, image/png" />
                      <div class="drag-text"> <h3>Drag and drop a file or select add Image</h3></div>
                    </div>

                    <!-- Error Message -->
                    <div class="feedback" id="if_invalid_image">This is a required field | File Format: PNG or JPG | File size: Max of 5 MB | 1 image upload only</div>
                    <div class="file-upload-content">
                      <img class="file-upload-image completion_i" id="product-image-img" src="#" alt="your image" />
                      <div class="image-title-wrap">
                        <button type="button" onclick="removeUpload()" id="remove-image" class="btn btn-outline-danger"><i class="fa fa-times"></i> Remove <span class="image-title">Uploaded Image</span></button>
                      </div>
                    </div>
                  </div>
                </div>

                <fieldset>
                <label class="grp-label"> <b>RETAIL</b> PRICE </label>
                
                <div class="form-group row">
                  <label class="col-sm-3 col-form-label">Stick Price
                    <small class="form-text text-muted">Suggested Selling Price</small>
                  </label>

                  <div class="col-sm-5"><input type="number" step=".01" class="form-control validator_c completion" min="1" id="stick_price_retail" required placeholder="PHP 0.00" oninput="this.value = Math.abs(this.value);if(this.value == 0){this.value = ''};if(this.value <= 0.99){this.value = ''};if(this.value > 9999){this.value = 9999};">
                  <div class="feedback">This is a required field</div>
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-sm-3 col-form-label">Pack Price
                    <small class="form-text text-muted">Suggested Selling Price</small>
                  </label>

                  <div class="col-sm-5"><input type="number" step=".01" class="form-control validator_c completion" min="1" id="pack_price_retail" required placeholder="PHP 0.00" oninput="this.value = Math.abs(this.value);if(this.value == 0){this.value = ''};if(this.value <= 0.99){this.value = ''};if(this.value > 9999){this.value = 9999}">
                  <div class="feedback">This is a required field</div>
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-sm-3 col-form-label">Ream Price
                    <small class="form-text text-muted">Acquisition Cost</small>
                  </label>

                  <div class="col-sm-5"><input type="number" step=".01" class="form-control validator_c completion" min="1" id="carton_price_retail" required placeholder="PHP 0.00" oninput="this.value = Math.abs(this.value);if(this.value == 0){this.value = ''};if(this.value <= 0.99){this.value = ''};if(this.value > 9999){this.value = 9999}">
                  <div class="feedback">This is a required field</div>
                  </div>
                </div>

                </fieldset>

                <fieldset>
                <label class="grp-label"> <b>WHOLESALE</b> PRICE </label>
                
                <div class="form-group row">
                  <label class="col-sm-3 col-form-label">Pack Price
                    <small class="form-text text-muted">Suggested Selling Price</small>
                  </label>

                  <div class="col-sm-5"><input type="number" step=".01" class="form-control validator_c completion" min="1" id="stick_price_wholesale" required placeholder="PHP 0.00" oninput="this.value = Math.abs(this.value);if(this.value == 0){this.value = ''};if(this.value <= 0.99){this.value = ''};if(this.value > 9999){this.value = 9999}">
                  <div class="feedback">This is a required field</div>
                  </div>
                </div>
                
                <div class="form-group row">
                  <label class="col-sm-3 col-form-label">Ream Price
                    <small class="form-text text-muted">Suggested Selling Price</small>
                  </label>

                  <div class="col-sm-5"><input type="number"  step=".01" class="form-control validator_c completion" min="1" id="pack_price_wholesale" required placeholder="PHP 0.00" oninput="this.value = Math.abs(this.value);if(this.value == 0){this.value = ''};if(this.value <= 0.99){this.value = ''};if(this.value > 9999){this.value = 9999}">
                  <div class="feedback">This is a required field</div>
                  </div>
                </div>

                <div class="form-group row">
                  <label class="col-sm-3 col-form-label">Ream Price
                    <small class="form-text text-muted">Acquisition Cost</small>
                  </label>

                  <div class="col-sm-5"><input type="number" class="form-control validator_c completion" min="1" maxlength="4" id="carton_price_wholesale" required placeholder="PHP 0.00" oninput="this.value = Math.abs(this.value);if(this.value == 0){this.value = ''};if(this.value <= 0.99){this.value = ''};if(this.value > 9999){this.value = 9999}">
                  <div class="feedback">This is a required field</div>
                  </div>
                </div>

                </fieldset>

                 <div class="form-group footer-buttons">
                  <a href="products" class="btn btn-cancel-round">Cancel</a>
                  <a class="btn btn-primary-round add-edit-btns disabled" id="add-product-btn">{{label_btn}}</a>
                </div>
              </form>
              
            </div>
          </div>
      </div>
    </div>

 <!-- MODAL - Save Product -->
  <div class="modal fade" id="addproduct" tabindex="-1" role="dialog" aria-labelledby="addproduct" aria-hidden="true" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog modal-md">
      <div class="modal-content">
        <div class="modal-body">
          <div class="spinner-border text-danger" role="status">
            <span class="sr-only">Loading...</span>
          </div>

          <h2>Saving..</h2>
          <p>Please wait.</p>

        </div>
      </div>
    </div>
  </div>
  <!-- END OF MODAL -->


  <!-- MODAL - Leave Page -->
  <div class="modal fade" id="leavepage" tabindex="-1" role="dialog" aria-labelledby="leavepage" aria-hidden="true">
    <div class="modal-dialog modal-md">
      <div class="modal-content">
        <div class="modal-body">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
          <i class="fas fa-exclamation-triangle"></i>
          <h2>Are you sure you want <br> to leave this page?</h2>

        </div>
        <div class="modal-footer">
             <button type="button" class="btn btn-cancel-round">No</button>
             <button type="button" class="btn btn-primary-round">Yes</button>
        </div>
      </div>
    </div>
  </div>
  <!-- END OF MODAL -->
